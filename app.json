{
  "expo": {
    "name": "SongVoter",
    "slug": "songvoter",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#1a1c1e",
    "primaryColor": "#a5c8ff",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1c1e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coflnet.songvoter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1c1e",
      },
      "package": "com.coflnet.songvoter",
      "intentFilters": [
        {
          "autoVerify": false,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "*.songvoter.party"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": ["FOREGROUND_SERVICE", "POST_NOTIFICATIONS", "WAKE_LOCK", "com.google.android.c2dm.permission.RECEIVE"],
      "versionCode": 3
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.coflnet.songvoter",
    "extra": {
      "eas": {
        "projectId": "5ed28b9f-440a-484b-a86d-bc8433942b3e"
      }
    },
    "owner": "coflnet",
    "updates": {
      "url": "https://u.expo.dev/5ed28b9f-440a-484b-a86d-bc8433942b3e"
    }
  }
}
